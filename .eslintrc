{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "accessor-pairs": 2,
        "block-scoped-var": 2,
        "camelcase": [2, {"properties": "always"}],
        "comma-dangle": [2, "always-multiline"],
        "complexity": 2,
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-location": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "indent": [2, 2],
        "no-alert": 2,
        "no-caller": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-parens": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 2,
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-spaced-func": 2,
        "no-throw-literal": 2,
        "no-undef": 2,
        "no-unexpected-multiline": 2,
        "no-unused-expressions": 2,
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-var": 2,
        "no-void": 2,
        "no-warning-comments": 2,
        "no-with": 2,
        "object-shorthand": 2,
        "prefer-const": 2,
        "prefer-reflect": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "quotes": [2, "double"],
        "radix": 2,
        "semi": [2, "always"],
        "wrap-iife": 2,
        "yoda": 2,
        // "no-empty-pattern": 2, // Not available until v1.7.0
        // "no-magic-numbers": 2,  // Not available until v1.7.0
    }
}
